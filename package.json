{
    "name": "spill",
    "version": "0.1.0",
    "description": "A Tumblr clone.",
    "keywords": [
        "tumblr",
        "blog",
        "tumblog",
        "social"
    ],
    "homepage": "@todo",
    "bugs": {
        "url": "http://"
    },
    "license": "MIT",
    "private": true,
    "author": "Kalyn Robinson <kalyndrobinson@gmail.com> (http://github.com/kalynrobinson)",
    "dependencies": {
        "antd": "^3.11.0",
        "lodash.groupby": "^4.6.0",
        "lodash.intersection": "^4.4.0",
        "lodash.sortby": "^4.7.0",
        "react": "^16.6.3",
        "react-dom": "^16.6.3",
        "react-router-breadcrumbs-hoc": "^2.1.5",
        "react-router-dom": "^4.3.1",
        "react-router-transition": "^1.2.1",
        "react-scripts": "2.1.1",
        "react-spring": "^6.1.10"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "docz:dev": "docz dev",
        "docz:build": "docz build",
        "commit": "git-cz"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged": {
        "*.{js,json,css,md}": [
            "prettier --write",
            "git add"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "commitizen": "^3.0.5",
        "commitlint": "^7.2.1",
        "cz-conventional-changelog": "2.1.0",
        "docz": "^0.12.15",
        "flow-bin": "^0.87.0",
        "husky": "^1.2.0",
        "lint-staged": "^8.1.0",
        "prettier": "^1.15.3"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
